{
  "name": "CRUD gsheets",
  "description": "perform crud in gsheets",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "crud sheets",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
